{
  "name": "@folio/marccat",
  "version": "1.2.0",
  "description": "FOLIO UI module for Cataloging",
  "repository": "folio-org/ui-marccat",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "main": "src/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node script/js/start",
    "status": "stripes status config/stripes.config.js",
    "build": "node ./script/js/build",
    "build:local": "stripes build config/stripes.config.js --languages en",
    "build:analizer": "stripes build config/stripes.config.js ./dist/analyze/ui-marccat-analyze --analyze --sourcemap",
    "lint": "yarn eslint && yarn stylelint",
    "test": "stripes test karma",
    "eslint": "eslint --fix ./",
    "stylelint": "stylelint --fix \"src/**/*.css\"",
    "flow": "flow status ./src/",
    "gen:authors": ". ./script/sh/authors.sh",
    "clean": "rm -rf lib/",
    "show:descriptor": "stripes mod descriptor --full",
    "release": "node script/ci/release.js"
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.9.0",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.1.0",
    "@folio/stripes-cli": "^1.6.0",
    "@folio/stripes-core": "^2.15.5",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "flow-bin": "^0.78.0",
    "mocha": "^5.2.0",
    "react": "~16.6.3",
    "react-dom": "~16.6.3",
    "react-redux": "~5.1.1",
    "redux": "^4.0.0",
    "sinon": "^7.0.0"
  },
  "dependencies": {
    "eslint-loader": "^2.1.0",
    "flow": "^0.2.3",
    "flow-remove-types": "^1.2.3",
    "namor": "1.1.1",
    "query-string": "^6.2.0",
    "react-beautiful-dnd": "9.0.2",
    "react-hot-loader": "^4.3.12",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.0.0",
    "redux-actions": "^2.6.4",
    "redux-form": "^7.4.2",
    "semafor": "^0.0.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.0.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^1.1.0",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "resolutions": {
    "**/domelementtype": "*",
    "**/isarray": "*",
    "**/redux": "*",
    "**/react-transition-group": "*",
    "**/warning": "*",
    "**/fbjs": "*",
    "**/@folio/stripes-react-hotkeys": "*",
    "**/@folio/stripes/components": "*",
    "**/hoist-non-react-statics": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "ui-marccat.app.title",
    "route": "/marccat",
    "defaultPopoverSize": "400px",
    "defaultPreviewWidth": "40%",
    "helpPage": "https://wiki.folio.org/pages/",
    "hasSettings": true,
    "okapiInterfaces": {
      "marccat": "1.0"
    },
    "icons": [
      {
        "name": "app",
        "alt": "FOLIO UI module for create, view and manage marc record",
        "title": "MARCcat"
      }
    ],
    "permissionSets": [
      {
        "permissionName": "module.marccat.enabled",
        "displayName": "UI: MARCcat module is enabled"
      },
      {
        "permissionName": "ui-marccat.view.search.records",
        "displayName": "MARCcat: Can search and view search results",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.view.search.detail",
        "displayName": "MARCcat: Can view details of search results",
        "subPermissions": [
          "ui-marccat.view.search.records",
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.view.browse.records",
        "displayName": "MARCcat: Can search and view browse results",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.view.browse.detail",
        "displayName": "MARCcat: Can view details of search results",
        "subPermissions": [
          "ui-marccat.view.browse.records",
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.settings.field-groupings",
        "displayName": "Settings (MARCcat): Can set up field group",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.settings.default.template",
        "displayName": "Settings (MARCcat): Can choose a default template",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.settings.local.authority,records",
        "displayName": "Settings (MARCcat): Can setup local authority records",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-marccat.settings.record.overlayrules",
        "displayName": "Settings (MARCcat): Can setting up rules for record overlay",
        "subPermissions": [
          "module.marccat.enabled"
        ],
        "visible": true
      }
    ]
  }
}
